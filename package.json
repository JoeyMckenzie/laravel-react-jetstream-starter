{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --ssr",
    "fmt": "pnpm biome format --write ./resources/js",
    "lint": "pnpm biome lint --write ./resources/js",
    "fix": "pnpm biome lint --write --unsafe ./resources/js",
    "check": "pnpm biome check --write ./resources/js",
    "ci": "pnpm run lint && pnpm run build"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "pnpm run fmt",
      "pnpm run lint"
    ],
    "**/*.php": [
      "composer run fmt",
      "composer run rector"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.1",
    "@prettier/plugin-php": "^0.18.9",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vue/server-renderer": "^3.3.13",
    "autoprefixer": "^10.4.19",
    "axios": "^0.26.1",
    "laravel-vite-plugin": "^0.7.8",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "vite": "^3.2.10"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@inertiajs/core": "^1.1.0",
    "@inertiajs/react": "^1.1.0",
    "@inertiajs/server": "^0.1.0",
    "classnames": "^2.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ziggy-js": "^1.8.2"
  }
}
